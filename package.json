{
  "name": "daisyui-vue2",
  "version": "0.0.0",
  "keywords": [
    "daisyui",
    "vue2"
  ],
  "exports": {
    "./nuxt/*": "./dist/nuxt.js",
    "./components/*": "./dist/components/*"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "siroc build",
    "build:watch": "siroc build --watch",
    "changeset": "changeset",
    "example:nuxt": "cd examples/nuxt && yarn dev",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/preset-env": "7.24.3",
    "@changesets/cli": "2.27.1",
    "@vue/test-utils": "1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "druxt": "0.24.0",
    "esbuild-jest": "0.5.0",
    "eslint": "8.57.0",
    "eslint-plugin-nuxt": "3.2.0",
    "jest": "27.5.1",
    "siroc": "0.16.0",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.7.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
